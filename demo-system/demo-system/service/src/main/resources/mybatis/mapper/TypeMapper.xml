<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.manage.system.dao.TypeMapper">

    <select id="findAllType" resultType="com.manage.system.dto.TypeDTO">
        select
            id,name,link_color,icon,parent_id
        from
            t_type
        where
            is_del = 0
    </select>
</mapper>